{
  "name": "sparkles",
  "version": "1.0.0",
  "description": "polish tracker",
  "main": "server.js",
  "dependencies": {
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "cheerio": "^1.0.0-rc.3",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mongoose": "^5.9.13",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "concurrently": "5.1.0"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "seed": "cd seed && node seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seiretsym/polishtracker.git"
  },
  "author": "kerwin hy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/seiretsym/polishtracker/issues"
  },
  "homepage": "https://github.com/seiretsym/polishtracker#readme"
}
